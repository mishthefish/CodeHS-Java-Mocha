public String doubleVowel(String str)
{
    str = str.replaceAll("a", "aa");
    str = str.replaceAll("A", "AA");
    str = str.replaceAll("e", "ee");
    str = str.replaceAll("E", "EE");
    str = str.replaceAll("I", "II");
    str = str.replaceAll("i", "ii");
    str = str.replaceAll("O", "OO");
    str = str.replaceAll("o", "oo");
    str = str.replaceAll("U", "UU");
    str = str.replaceAll("u", "uu");
    return str;
}
